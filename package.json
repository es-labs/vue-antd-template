{
  "name": "vue-antd-template",
  "description": "vue SPA + antdv + vite template",
  "version": "0.0.1",
  "private": true,
  "author": "aaronjxz@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "vite build --config src/apps/vite.config.js",
    "dev": "vite --config src/apps/vite.config.js",
    "dev:mocked": "vite --config src/apps/vite.config.js --mode mocked",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "prettier:check": "prettier --check src/**/*.{js,jsx,json,vue}",
    "prettier:write": "prettier --write src/**/*.{js,jsx,json,vue}",
    "test:e2e": "echo \"Remember to run frontend and backend before test...\" && playwright test"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@es-labs/esm": "^0.0.21",
    "@sentry/vue": "^7.74.0",
    "ant-design-vue": "^4.0.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "msw": "^2.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.11"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
